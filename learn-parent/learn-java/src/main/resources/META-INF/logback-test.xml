<configuration>
    <contextName>eznla-posloan-service</contextName>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%mdc{uuid}][%level]%logger{0}-%msg%n</pattern>
        </encoder>
    </appender>
    <logger name="com.bocsoft" level="DEBUG"/>
    <!--<logger name="com.alibaba.dubbo" level="DEBUG"/> -->
    <root>
        <level value="INFO"/>
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>